<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="gnome-vagrant-indicator">
    <schema path="/org/gnome/shell/extensions/gnome-vagrant-indicator/" id="org.gnome.shell.extensions.gnome-vagrant-indicator">
        <key name="prefs-default-width" type="i">
            <default>0</default>
            <summary>Preferences window default width</summary>
        </key>
        <key name="prefs-default-height" type="i">
            <default>0</default>
            <summary>Preferences window default height</summary>
        </key>
        <key name="notifications" type="b">
            <default>true</default>
            <summary>Show notifications</summary>
            <description>Display notification on vagrant machine state change</description>
        </key>
        <key name="machine-full-path" type="b">
            <default>false</default>
            <summary>Show machine full path</summary>
            <description>Show machine full path as instance name</description>
        </key>
        <key name="machine-name" type="b">
            <default>false</default>
            <summary>Show machine name</summary>
            <description>Show machine name next to instance name</description>
        </key>
        <key name="post-terminal-action" type="s">
            <default>'NONE'</default>
            <summary>Post terminal action</summary>
            <description>Action after `vagrant` command execution</description>
        </key>
        <key name="auto-global-status-prune" type="b">
            <default>false</default>
            <summary>Refresh machines status at startup</summary>
            <description>Refresh machines status at startup</description>
        </key>
        <key name="display-system-none" type="b">
            <default>false</default>
            <summary>Disabled</summary>
            <description>Disabled</description>
        </key>
        <key name="display-system-terminal" type="b">
            <default>true</default>
            <summary>Open in Terminal</summary>
            <description>Display Open in Terminal system menu</description>
        </key>
        <key name="display-system-file-manager" type="b">
            <default>true</default>
            <summary>Open in File Manager</summary>
            <description>Display Open in File Manager system menu</description>
        </key>
        <key name="display-system-vagrantfile" type="b">
            <default>true</default>
            <summary>Edit Vagrantfile</summary>
            <description>Display Edit Vagrantfile system menu</description>
        </key>
        <key name="display-vagrant-none" type="b">
            <default>false</default>
            <summary>Disabled</summary>
            <description>Disabled</description>
        </key>
        <key name="display-vagrant-up" type="b">
            <default>true</default>
            <summary>Up</summary>
            <description>Display menu for `vagrant up` command</description>
        </key>
        <key name="display-vagrant-up-provision" type="b">
            <default>false</default>
            <summary>Up and Provision</summary>
            <description>Display menu for `vagrant up --provision` command</description>
        </key>
        <key name="display-vagrant-up-ssh" type="b">
            <default>false</default>
            <summary>Up asd SSH</summary>
            <description>Display menu for `vagrant up; vagrant ssh` command</description>
        </key>
        <key name="display-vagrant-up-rdp" type="b">
            <default>false</default>
            <summary>Up and RDP</summary>
            <description>Display menu for `vagrant up; vagrant rdp` command</description>
        </key>
        <key name="display-vagrant-provision" type="b">
            <default>false</default>
            <summary>Provision</summary>
            <description>Display menu for `vagrant provision` command</description>
        </key>
        <key name="display-vagrant-ssh" type="b">
            <default>true</default>
            <summary>SSH</summary>
            <description>Display menu for `vagrant ssh` command</description>
        </key>
        <key name="display-vagrant-rdp" type="b">
            <default>false</default>
            <summary>RDP</summary>
            <description>Display menu for `vagrant rdp` command</description>
        </key>
        <key name="display-vagrant-resume" type="b">
            <default>false</default>
            <summary>Resume</summary>
            <description>Display menu for `vagrant resume` command</description>
        </key>
        <key name="display-vagrant-suspend" type="b">
            <default>false</default>
            <summary>Suspend</summary>
            <description>Display menu for `vagrant suspend` command</description>
        </key>
        <key name="display-vagrant-halt" type="b">
            <default>true</default>
            <summary>Halt</summary>
            <description>Display menu for `vagrant halt` command</description>
        </key>
        <key name="display-vagrant-destroy" type="b">
            <default>false</default>
            <summary>Destroy</summary>
            <description>Display menu for `vagrant destroy` command</description>
        </key>
        <key name="display-vagrant-destroy-force" type="b">
            <default>false</default>
            <summary>Disabled</summary>
            <description>Disabled</description>
        </key>
